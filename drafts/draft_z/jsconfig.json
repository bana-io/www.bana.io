{
  "compilerOptions": {
    "baseUrl": "./docs/.vuepress",
    //   "module": "commonjs",
    //   "target": "es6",
    //   "lib": [
    //     "esnext",
    //     "es6"
    //   ]
    "checkJs": true,
    // "module": "commonjs",
    // "target": "es6",
    "types": [
      "webpack-env",
      "node",
    ],
    "target": "esnext",
    "declaration": true,
    "esModuleInterop": true,
    "pretty": true,
    "moduleResolution": "node",
    "lib": [
      "es2015",
      "es2016.array.include"
    ],
    "allowSyntheticDefaultImports": true,
    "stripInternal": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "module": "commonjs",
    "outDir": "lib",
    "typeRoots": [
      "node_modules/@types"
    ],
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "dist",
    "dist/",
    "dist/**/*",
    "docs/.temp",
    "docs/.temp/",
    "docs/.temp/**/*",
    "docs/.cache",
    "docs/.cache/",
    "docs/.cache/**/*",
    // "docs/*",
    // "docs/**",
    // "docs/**/*",
    // "./docs/*",
    // "./docs/**",
    // "./docs/**/*",
  ],
  "include": [
    "./docs/.vuepress/*",
    "./docs/.vuepress/*.js",
    "./docs/.vuepress/**/*",
    "./docs/.vuepress/**/*.js",
    "docs/.vuepress/*",
    "docs/.vuepress/*.js",
    "docs/.vuepress/**/*",
    "docs/.vuepress/**/*.js",
  ],
}
