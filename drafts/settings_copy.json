{
  "[Log]": {
    "editor.fontSize": 12
  },
  "[git-commit]": {
    "editor.rulers": [
      50,
      72
    ]
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.wordWrap": "on"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "on"
  },
  "[shellscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.autoIndent": "advanced",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": false,
  "cSpell.checkLimit": 9007199254740991,
  "cSpell.enableFiletypes": [
    "!.*.log"
  ],
  "cSpell.language": "en-GB",
  "cSpell.logLevel": "Error",
  "cSpell.maxDuplicateProblems": 2,
  "cSpell.maxNumberOfProblems": 5,
  "cSpell.minWordLength": 1,
  "cSpell.spellCheckDelayMs": 1000,
  "cSpell.words": [
    "Bana",
    "Marrakech",
    "kubernetes"
  ],
  "diffEditor.renderIndicators": true,
  "diffEditor.renderSideBySide": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.codeLens": false,
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "block-outline",
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Iosevka Fixed Extended','Iosevka-Extended','monospace', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    80,
    100
  ],
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "inline",
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.validate": [
    "json",
    "jsonc"
  ],
  "explorer.confirmDragAndDrop": true,
  "explorer.openEditors.visible": 5,
  "files.associations": {
    "*_profile": "shellscript",
    ".*ignore": "ignore"
  },
  "files.autoSave": "afterDelay",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/.temp": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/**": true
  },
  "markdownlint.run": "onSave",
  "problems.showCurrentInStatus": true,
  "scm.defaultViewMode": "tree",
  "search.actionsPosition": "right",
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.sortOrder": "countDescending",
  "terminal.explorerKind": "external",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#D50000",
    "editorIndentGuide.activeBackground": "#EF9A9A"
  },
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "medium",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": false,
  "workbench.settings.useSplitJSON": false,
  "workbench.tree.indent": 16,
  "yaml.format.enable": true
}
